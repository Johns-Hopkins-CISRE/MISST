    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run MISST JH Example Trainer",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "program": "train.py",
            "cwd": "${workspaceFolder}/examples/training/"
        },
        {
            "name": "Run MISST Generator",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "module": "misst",
            "args": ["--mode", "generate"]
        }
    ]
}